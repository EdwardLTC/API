@page
@model EcommerceAPI.Controller.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Clothe[0].Des)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Clothe[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Clothe[0].IdCategoryNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Clothe[0].IdsellerNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Clothe) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Des)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCategoryNavigation.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdsellerNavigation.Id)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
